    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "pla converter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pla_converter.py",
            "python": "/usr/bin/python3",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/pla/raw/distributed_lock_maxheld/distributed_lock_maxheld_1_1.txt"
            ],
        },
        {
            "name": "fr",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ic3po/top.py",
            "console": "integratedTerminal",
            "subProcess": true,
            "args": [
                "-m",
                "frpo",
                "--init",
                "-1",
                "${workspaceFolder}/ivybench/ex/vmt/toy_consensus.vmt",
                "<",
                "${workspaceFolder}/mock.txt"
            ],
        },
        {
            "name": "ring not dead",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ic3po/top.py",
            "console": "integratedTerminal",
            "subProcess": true,
            "args": [
                "-m",
                "frpo",
                "${workspaceFolder}/ivybench/ex/vmt/ring_not_dead.vmt",
                "<",
                "${workspaceFolder}/mock.txt"
            ],
        },
    ]
}