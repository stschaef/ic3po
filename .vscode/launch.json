    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "pla converter",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/pla_converter.py",
            "python": "/usr/bin/python3",
            "console": "integratedTerminal",
            "args": [
                "${workspaceFolder}/pla/raw/distributed_lock_maxheld/distributed_lock_maxheld_1_1.txt"
            ],
        },
        {
            "name": "fr",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ic3po/top.py",
            "console": "integratedTerminal",
            "subProcess": true,
            "args": [
                "-m",
                "frpo",
                "--init",
                "-1",
                "${workspaceFolder}/ivybench/ex/vmt/toy_consensus.vmt",
                "<",
                "${workspaceFolder}/mock.txt"
            ],
        },
        {
            "name": "tc",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ic3po/top.py",
            "console": "integratedTerminal",
            "subProcess": true,
            "args": [
                "-o",
                "foo",
                "-n",
                "bar",
                "${workspaceFolder}/ivybench/tla/vmt/TCommit.vmt",
                "<",
                "${workspaceFolder}/mock.txt"
            ],
        },
        {
            "name": "tl",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ic3po/top.py",
            "console": "integratedTerminal",
            "subProcess": true,
            "args": [
                "-o",
                "foo",
                "-n",
                "bar",
                "${workspaceFolder}/toy_lock/normal/toy_lock.vmt",
                "<",
                "${workspaceFolder}/mock.txt"
            ],
        },
        {
            "name": "toy_lock",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/ic3po/top.py",
            "console": "integratedTerminal",
            "subProcess": true,
            "args": [
                "-m",
                "satfr",
                "${workspaceFolder}/toy_lock/abstract/toy_lock.vmt",
                "<",
                "${workspaceFolder}/mock.txt"
            ],
        },
    ]
